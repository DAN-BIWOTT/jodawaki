{
  "name": "@landing/018-gatsby",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.0.1",
  "author": "RedQ, Inc.",
  "dependencies": {
    "framer-motion": "^2.9.4",
    "gatsby": "^4.0.0-next.0",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-google-analytics": "^2.4.0",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^2.5.1",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.7.0",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.4.0",
    "gatsby-source-mongodb": "^4.3.0",
    "gatsby-transformer-sharp": "^2.5.21",
    "polished": "^4.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-masonry-component": "^6.2.1",
    "react-responsive": "^8.1.0",
    "react-scroll": "^1.8.1",
    "react-slick": "^0.27.12",
    "react-stickynode": "^3.0.4",
    "redux": "^4.1.2",
    "sequelize": "^6.12.0-beta.1",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@iconify/react": "^3.1.0",
    "prettier": "^2.1.1",
    "styled-components": "^5.3.3"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "server": "node server.js",
    "start": "concurrently \"gatsby develop\" \"npm run server\" "
  },
  "engines": {
    "node": ">=16"
  },
  "nodemonConfig": {
    "watch": [
      "server.js"
    ],
    "ext": "js",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  }
